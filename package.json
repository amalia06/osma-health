{
  "name": "osma-health",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/center-of-mass": "^6.0.1",
    "@turf/helpers": "^6.1.3",
    "d3-scale": "^2.0.0",
    "d3-scale-chromatic": "^1.2.0",
    "date-fns": "^1.29.0",
    "lodash.upperfirst": "^4.3.1",
    "mapbox-gl": "^0.44.2",
    "numeral": "^2.0.6",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "homepage": ".",
  "scripts": {
    "postinstall": "npm run preprocess-css",
    "preprocess-css": "shx cp -R node_modules/jeet src/styles/scss/",
    "build-css": "node-sass-chokidar src/styles/scss/ -o src/styles/css/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss/ -o src/styles/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jeet": "^7.2.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "shx": "^0.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
